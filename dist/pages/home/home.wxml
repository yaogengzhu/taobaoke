<block wx:if="{{$taroCompReady}}">
    <view>
        <view class="lunbotu">
            <swiper class="test-h" indicator-color="#333" indicator-active-color="#1296db" circular="{{true}}" indicator-dots="{{true}}" autoplay="{{true}}">
                <swiper-item>
                    <view class="demo-text">
                        <image src="https://aecpm.alicdn.com/simba/img/TB1CWf9KpXXXXbuXpXXSutbFXXX.jpg_q50.jpg" class="imgSrc"></image>
                    </view>
                </swiper-item>
                <swiper-item>
                    <view class="demo-text">
                        <image src="https://aecpm.alicdn.com/simba/img/TB14ab1KpXXXXclXFXXSutbFXXX.jpg_q50.jpg" class="imgSrc"></image>
                    </view>
                </swiper-item>
                <swiper-item>
                    <view class="demo-text">
                        <image src="https://gw.alicdn.com/imgextra/i2/78/O1CN019lSGp71CRlRdIPVMB_!!78-0-lubanu.jpg" class="imgSrc"></image>
                    </view>
                </swiper-item>
            </swiper>
        </view>
        <view>
            <at-noticebar compid="{{$compid__43}}"><text class="tip">特别通知：小程序正在开发中</text>
            </at-noticebar>
        </view>
        <template is="renderNav" data="{{...anonymousState__temp}}"></template>
        <view class="hot">热<text class="line">/</text>搜<text class="line">/</text>榜<text class="line">/</text>推<text class="line">/</text>荐</view>
        <view class="goodsBox" wx:key="num_iid" wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="_anonIdx2">
            <view class="left">
                <image src="{{item.pict_url}}" class="imgSrc"></image>
            </view>
            <view class="right">
                <view class="title">{{item.title}}</view>
                <view class="price">
                    <view class="oldPrice">¥{{item.reserve_price}}</view>
                    <view class="newPrice">¥{{item.zk_final_price}}</view>
                </view>
                <view class="bottom">
                    <view>{{item.nick}}</view>
                    <view>{{item.volume}}人付款</view>
                    <view>{{item.provcity}}</view>
                </view>
            </view>
        </view>
    </view>
</block>